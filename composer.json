{
    "name": "argentcrusade/crud-kit",
    "description": "CRUD Kit package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-fractal": "^6.0",
        "argentcrusade/repository": "^5.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0",
        "laravel/legacy-factories": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "ArgentCrusade\\CrudKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArgentCrusade\\CrudKit\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
