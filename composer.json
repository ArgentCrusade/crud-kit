{
    "name": "argentcrusade/crud-kit",
    "description": "CRUD Kit package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "spatie/laravel-fractal": "^5.2",
        "argentcrusade/repository": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
