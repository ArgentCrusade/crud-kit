{
    "name": "argentcrusade/crud-kit",
    "description": "CRUD Kit package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "spatie/laravel-fractal": "^5.8",
        "argentcrusade/repository": "^3.2",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.3",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "ArgentCrusade\\CrudKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArgentCrusade\\CrudKit\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
